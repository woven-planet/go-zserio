{{ $scope := .pkg }}
{{ $field_name := .field_name }}
{{ $array := .array }}
{{- if $array.IsPacked }}
    {{ $field_name }}.IsPacked = true
{{- end }}
{{- if $array.Length }}
    {{ $field_name }}.FixedSize = int({{ goExpression $scope $array.Length }})
{{- else }}
    {{ $field_name }}.IsAuto = true
{{- end }}